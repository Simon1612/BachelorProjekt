@using Microsoft.AspNetCore.Http
@using Microsoft.AspNetCore.Server.Kestrel.Internal;
<!DOCTYPE html>
<html>
@{
    Layout = "../_Layout.cshtml";
    ViewBag.Title = "Dental Research App";
    var sourceSite = Url.ActionContext.HttpContext.Request.Headers["Referer"];
}
<head>
    <meta charset="utf-8" />
    <title>Survey Results</title>
    <link rel="stylesheet" href="~/css/3rdParty/bootstrap.min.css" />
    <script type="text/javascript" src="~/js/3rdParty/jquery.min.js"></script>
    <script type="text/javascript" src="~/js/3rdParty/jquery.dataTables.min.js"></script>
    <script src="~/js/3rdParty/bootstrap.min.js"></script>
    <script src="~/js/3rdParty/knockout-debug.js"></script>
    <script src="~/js/3rdParty/survey.ko.js"></script>
    <link rel="stylesheet" href="~/css/3rdParty/survey.css" />
    <link rel="stylesheet" href="~/css/3rdParty/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="~/css/Index.css" />
</head>

<body>
    <div class="survey-page-header">
        <div class="sv_main survey-page-header-content">
            <input type="button" value="&lt Back" class="btn btn-primary" onclick="location.href='@sourceSite'"/>
        </div>
    </div>
    <div class="sv_main sv_frame sv_default_css">
        <div class="sv_custom_header"></div>
        <div class="sv_container">
            <div class="sv_header">
                <h3 data-bind="text: surveyId + ' results'"></h3>
            </div>
            <div class="sv_body">
                <table id="resultsTable" class="table table-striped table-bordered"></table>
            </div>
        </div>
    </div>
    <script src="/js/results.js">
    </script>
</body>

</html>