@model DentalResearchApp.Models.StudySessionModel
@{
    ViewBag.Title = "Session Details";
    Layout = "~/Views/_Layout.cshtml";
}
<html>
<head>
    <meta charset="utf-8" />

    <link rel="stylesheet" href="~/css/3rdParty/bootstrap.min.css" />
    <script type="text/javascript" src="~/js/3rdParty/jquery.min.js"></script>
    <script src="~/js/3rdParty/bootstrap.min.js"></script>

    <link rel="stylesheet" href="/css/Index.css" type="text/css" />
    <title>Dental Research App - Session Details</title>
</head>

<body>
    <div class="col-lg-12 col-md-12 col-xs-12">
        <h1 class="secondHeadline" style="text-align: center">Study: @ViewBag.studyName</h1>
        <h2 class="secondHeadline" style="text-align: center">Session: @Model.SessionName</h2>
        <hr />
            <h4 class="secondHeadline" style="font-weight: bold">Related Surveys</h4>
            <table class="customTable">
                <thead>
                    <tr>
                        <th class="customTh">Surveys</th>
                    </tr>
                </thead>
                @if (Model.SelectedSurveys.Any())
                {
                <tbody>
                    @foreach (var item in Model.AllSurveys)
                    {
                    <tr>
                        <td class="customTd">
                            @item @*Todo: Hvor fører et click hen?*@
                        </td>
                    </tr>
                    }
                </tbody>
                }
            </table>
        </div>
<hr />
<div class="col-lg-12 col-md-12 col-sm-12" style="text-align: center">
    <button class="btn btn-primary">Add Survey to Session</button> @*Todo: Find ud af hvordan denne knap skal fungere*@
</div>
</body>
</html>